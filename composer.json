{
	"name": "rajan-lama/simple-rest-form",
	"description": "Simple Rest Form WordPress Plugins",
	"License": "GPL-3-0",
	"version": "1.0.0",
	"keywords": [
		"wordpress",
		"plugin"
	],
	"requires": {
		"php": ">=5.6.0"
	},
	"homepage": "https://github.com/rajan-lama/simple-rest-form",
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.0",
		"phpcompatibility/php-compatibility": "*"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"autoload": {
		"psr-4": {
			"simple-rest-form\\": "/"
		}
	},
	"authors": [
		{
			"name": "Rajan Lama",
			"email": "rajan.lama786@gmail.com"
		}
	]
}
