{
	"name": "simple-rest-form",
	"version": "1.0.0",
	"description": "Simple rest form plugin packages",
	"main": "main.js",
	"scripts": {
		"prod": "cross-env NODE_ENV=production webpack --mode production --progress",
		"dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
		"clean": "rm -rf assets/build/*",
		"svg": "svgr -d assets/src/js/icons assets/src/icons",
		"lint:php": "vendor\\bin\\phpcs",
		"lint:php:fix": "vendor\\bin\\phpcbf",
		"lint": "eslint assets/src/js/*",
		"lint:fix": "eslint --fix assets/src/js/*",
		"lint-css": "stylelint 'assets/src/sass/**/*.scss'",
		"lint-css:fix": "npm run lint-css -- --fix",
		"precommit": "npm run lint:php && npm run lint-css:fix && npm run lint:fix && npm run prod"
	},
	"keywords": [
		"wordpress",
		"themes"
	],
	"author": "Rajan Lama",
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"@babel/core": "^7.24.8",
		"@babel/preset-env": "^7.24.8",
		"@babel/preset-react": "^7.24.7",
		"@svgr/cli": "^6.5.1",
		"@wordpress/base-styles": "^4.49.0",
		"@wordpress/block-editor": "^8.6.0",
		"@wordpress/blocks": "^11.21.0",
		"@wordpress/components": "^19.17.0",
		"@wordpress/compose": "^5.20.0",
		"@wordpress/data": "^6.15.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^3.7.0",
		"@wordpress/element": "^4.20.0",
		"@wordpress/eslint-plugin": "^11.1.0",
		"@wordpress/hooks": "^3.58.0",
		"@wordpress/i18n": "^4.58.0",
		"@wordpress/icons": "^8.4.0",
		"@wordpress/server-side-render": "^3.20.0",
		"babel-loader": "^8.3.0",
		"clean-webpack-plugin": "^4.0.0",
		"copy-webpack-plugin": "^10.2.4",
		"cross-env": "^7.0.3",
		"css-loader": "^6.11.0",
		"cssnano": "^5.1.15",
		"eslint": "^8.57.0",
		"eslint-config-wordpress": "^2.0.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-jsdoc": "^38.1.6",
		"file-loader": "^6.2.0",
		"lodash": "^4.17.21",
		"mini-css-extract-plugin": "^2.9.0",
		"node-sass": "^7.0.1",
		"optimize-css-assets-webpack-plugin": "^6.0.1",
		"sass-loader": "^12.6.0",
		"sass-mq": "^6.0.0",
		"style-loader": "^3.3.4",
		"stylelint": "^14.16.1",
		"stylelint-config-wordpress": "^17.0.0",
		"stylelint-webpack-plugin": "^3.3.0",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack": "^5.93.0",
		"webpack-cli": "^4.10.0"
	},
	"browserslist": [
		"defaults"
	],
	"dependencies": {}
}
